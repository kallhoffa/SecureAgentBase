{
  "name": "argbase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --pool=threads",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "check": "npm run test:ci && npm run lint && npm run build",
    "e2e": "playwright test",
    "e2e:ci": "playwright test --config=playwright.ci.config.js",
    "e2e:smoke": "playwright test tests/e2e/smoke.spec.js",
    "e2e:smoke:ci": "playwright test tests/e2e/smoke.spec.js --config=playwright.ci.config.js"
  },
  "dependencies": {
    "@sentry/react": "^10.40.0",
    "firebase": "^11.0.1",
    "lucide-react": "^0.454.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "tailwindcss": "^3.4.19",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@sentry/vite-plugin": "^5.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.24",
    "axe-core": "^4.11.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "husky": "^9.1.7",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  }
}
